<% content_for :head do %>
<title>Austin Tie Dye Co: Admin Login</title>
<%end%>


<% content_for :body do %>
<div class="row">
	<div class="col-sm-10">
		<h2>Admin Login.</h2>
	</div>
</div>
<div class="row">
	<div class="col-sm-10">
	<%= simple_form_for @admin, url: admins_login_path do |f| %>
  	<%= f.input :email %>
  	<%= f.input :password %>
  	<%= f.button :submit, value: "Log In"%>
  </div>
	<%end%>

<%end%>
